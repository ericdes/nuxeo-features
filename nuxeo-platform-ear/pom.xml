  <build>
    <plugins>
      <!-- build zip artifacts with resources -->
      <plugin>
        <artifactId>maven-assembly-plugin</artifactId>
        <version>2.2-beta-2</version>
        <configuration>
          <descriptors>
            <descriptor>src/main/assemble/resources/common.xml</descriptor>
            <descriptor>src/main/assemble/resources/core.xml</descriptor>
            <descriptor>
              src/main/assemble/resources/core-sync-index.xml
            </descriptor>
            <descriptor>
              src/main/assemble/resources/platform-stateful.xml
            </descriptor>
            <descriptor>src/main/assemble/resources/search.xml</descriptor>
            <descriptor>src/main/assemble/resources/webplatform.xml</descriptor>
            <descriptor>
              src/main/assemble/resources/web-stateless.xml
            </descriptor>
            <descriptor>src/main/assemble/resources/mono.xml</descriptor>
          </descriptors>
        </configuration>
        <executions>
          <execution>
            <id>make-resources</id>
            <phase>package</phase>
            <goals>
              <goal>attached</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
      <!-- Required for the "jboss-sar" type resolution -->
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>jboss-packaging-maven-plugin</artifactId>
        <version>2.0-beta-1</version> <!-- required for enabling mevenide -->
        <extensions>true</extensions>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-nuxeo-plugin</artifactId>
        <extensions>false</extensions>
        <executions>
          <execution>
            <id>assemble-ear</id>
            <phase>package</phase>
            <goals>
              <goal>assembly</goal>
            </goals>
            <configuration>
              <runPreprocessor>false</runPreprocessor>
              <format>directory</format>
              <outputDirectory>target</outputDirectory>
              <targetFile>${nuxeo.ear.assembly}.ear</targetFile>
              <descriptor>
                ${basedir}/src/main/assemble/${nuxeo.ear.assembly}.xml
              </descriptor>
            </configuration>
          </execution>
        </executions>
      </plugin>
      <!--plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>build-helper-maven-plugin</artifactId>
        <executions>
        <execution>
        <id>attach-artifacts</id>
        <phase>package</phase>
        <goals>
        <goal>attach-artifact</goal>
        </goals>
        <configuration>
        <artifacts>
        <artifact>
        <file>
        ${project.build.directory}/nuxeo-platform-ear-${pom.version}.ear
        </file>
        <type>ear</type>
        </artifact>
        </artifacts>
        </configuration>
        </execution>
        </executions>
        </plugin-->
      <!-- copy nuxeo libraries that must go into $JBOSS/server/default/lib/ -->
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-dependency-plugin</artifactId>
        <configuration>
          <outputDirectory>${jboss.lib}</outputDirectory>
          <artifactItems>
            <artifactItem>
              <groupId>org.nuxeo.common</groupId>
              <artifactId>nuxeo-common</artifactId>
              <version>${nuxeo.common.version}</version>
              <overWrite>true</overWrite>
            </artifactItem>
            <artifactItem>
              <groupId>org.nuxeo.runtime</groupId>
              <artifactId>nuxeo-runtime-jboss-extensions</artifactId>
              <version>${nuxeo.runtime.version}</version>
              <overWrite>true</overWrite>
            </artifactItem>
            <artifactItem>
              <groupId>org.apache.derby</groupId>
              <artifactId>derby</artifactId>
            </artifactItem>
          </artifactItems>
        </configuration>
        <executions>
          <execution>
            <id>copy-installed</id>
            <phase>package</phase>
            <goals>
              <goal>copy</goal>
            </goals>
            <configuration>
              <outputDirectory>${project.build.directory}/lib</outputDirectory>
            </configuration>
          </execution>
          <execution>
            <id>copy-postgresql-jdbc-driver</id>
            <phase>package</phase>
            <goals>
              <goal>copy</goal>
            </goals>
            <configuration>
              <outputDirectory>
                ${project.build.directory}/postgresql-jdbc-lib
              </outputDirectory>
              <artifactItems>
