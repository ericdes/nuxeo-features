<?xml version="1.0"?>
<fragment>
  <extension target="jboss-app#MODULE">
		<module>
			<service>${bundle.fileName}</service>
		</module>
	</extension>
	<extension target="faces-config#NAVIGATION">
  <navigation-case>
    <from-outcome>workflow_create_view</from-outcome>
    <to-view-id>/workflow_create_view.xhtml</to-view-id>
    <redirect/>
  </navigation-case>
	</extension>
	<install>
	  <unzip from="${bundle.fileName}" to="/">
	    <include>nuxeo.war/**</include>
	  </unzip>
	</install>
</fragment>
