<?xml version="1.0"?>
<component name="org.nuxeo.ecm.platform.jbpm.TypeContrib">
	<require>org.nuxeo.ecm.core.CoreExtensions
	</require>
	<extension target="org.nuxeo.ecm.core.schema.TypeService"
		point="schema">
		<schema name="task" src="schemas/task.xsd" prefix="task" />
		<schema name="workflow" src="schemas/workflow.xsd" prefix="wf" />
	</extension>
	<extension target="org.nuxeo.ecm.core.schema.TypeService"
		point="doctype">
		<doctype name="TaskFolder" extends="Document">
			<schema name="common" />
			<schema name="dublincore" />
			<facet name="Folderish" />
		</doctype>
		<doctype name="WorkflowFolder" extends="Document">
			<schema name="common" />
			<schema name="dublincore" />
			<facet name="Folderish" />
		</doctype>
		<doctype name="Task" extends="Document">
			<schema name="task" />
			<schema name="dublincore" />
		</doctype>
		<doctype name="Workflow" extends="Document">
			<schema name="workflow" />
			<schema name="dublincore" />
		</doctype>
	</extension>
	<extension target="org.nuxeo.ecm.core.lifecycle.LifeCycleService"
		point="types">
		<types>
			<type name="TaskFolder">default</type>
			<type name="Task">default</type>
			<type name="WorkflowFolder">default</type>
			<type name="Workflow">default</type>
		</types>
	</extension>
</component>