<div class="contextualLinks">
  <h4>${Context.getMessage("title.contextual.links.pages")}</h4>
  <ul>  
    <#list nxthemesInfo.model.items as item>
      <div class="contextualLinkBlock">
        <div class="contextualLink"><a href="${item.link}" target="_blank"> ${item.title} &nbsp; </a></div>
        <div class="contextualLinkText">${item.description}</div>
      </div>
    </#list>
  </ul>
</div>
